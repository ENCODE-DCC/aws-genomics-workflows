{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "GetObjectAndAcl",
            "Effect": "Allow",
            "Principal": "*",
            "Action": [
                "s3:GetObject",
                "s3:GetObjectAcl"
            ],
            "Resource": "arn:aws:s3:::caper-aws-genomics-workflows/*"
        },
        {
            "Sid": "ListBucket",
            "Effect": "Allow",
            "Principal": "*",
            "Action": "s3:ListBucket",
            "Resource": "arn:aws:s3:::caper-aws-genomics-workflows"
        }
    ]
}

